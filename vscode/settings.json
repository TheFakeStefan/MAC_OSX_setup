{
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "psi-header.config": {
    "forceToTop": true,
    "blankLinesAfter": 1
  },
  "psi-header.changes-tracking": {
    "isActive": true,
    "modAuthor": "Modified By: ",
    "modDate": "Last Modified: ",
    "modDateFormat": "date",
    "exclude": [".key", ".csr", ".crt", ".csv"],
    "excludeGlob": [
      "**/*.csv",
      "**/*.json.j2",
      "**/*.json",
      "**/*.log",
      "**/*.logstash.conf",
      "**/*.md",
      "**/*.ps1",
      "**/*.txt",
      "**/*.yml.j2"
    ],
    "autoHeader": "autoSave",
    "enforceHeader": true
  },
  "psi-header.variables": [
    ["company", "XXX"],
    ["author", "Stefan"],
    ["initials", "XXX"],
    ["authoremail", "XXX"],
    ["projectname", "XXX"]
  ],
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "File: <<filename>>",
        "Local Path: <<filepath>>",
        "-----",
        "Project: <<projectname>>",
        "File Created: <<filecreated('YYYY-MM-DD hh:mm:ss a')>>",
        "Author: <<author>>, (<<authoremail>>)",
        "-----",
        "Last Modified: <<dateformat('YYYY-MM-DD hh:mm:ss a')>>",
        "Modified By: <<author>>, (<<authoremail>>>)",
        "-----",
        "History:",
        "-----",
        "<<year>> - <<author>>, <<company>>"
      ],
      "changeLogCaption": "History:",
      "changeLogHeaderLineCount": 0,
      "changeLogEntryTemplate": [
        "<<dateformat('YYYY-MM-DD hh:mm:ss a')>> <<initials>>: "
      ]
    }
  ],
  "psi-header.lang-config": [
    {
      "language": "Markdown",
      "prefix": "// ",
      "begin": "<!---",
      "end": "-->"
    },
    {
      "language": "*",
      "begin": "#####################################################################",
      "end": "#####################################################################",
      "prefix": "# "
    }
  ],
  "files.associations": {
    "*.yml": "ansible",
    "*.logstash": "logstash",
    "*.md": "markdown"
  },
  "http.proxyStrictSSL": false,
  "sync.gist": "6835ae78925ff2bcf023853db84896fb",
  "editor.fontFamily": "Hack",
  "editor.tabSize": 2,
  "[json]": {
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "git.enableSmartCommit": true,
  "git.autofetch": true
}
